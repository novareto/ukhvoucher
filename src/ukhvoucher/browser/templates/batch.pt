<div class="batch">
  <ul>
    <li i18n:translate="">Seiten&nbsp;&nbsp;</li>
    <li tal:repeat="info batch.batches()">
      <a class="page"
	 tal:condition="info['current'] == False"
	 tal:attributes="href info['url']"
	 tal:content="info['id']" />
	
      <a class="page" name="current-page"
	 tal:condition="info['current'] == True"
	 tal:content="info['id']" />
    </li>
  </ul>
</div>
